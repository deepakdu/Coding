class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        int l=nums.size();
        if(l==1){
            return l;
        }
        for(int i=1;i<nums.size();i++){
            if(nums[i]==nums[i-1]){
                nums.erase(nums.begin()+i-1);
                i--;
            }
        }
        int s=nums.size();
    return s;
    }
};
