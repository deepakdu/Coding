class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        unordered_map<int,int>CompSum;
        unordered_map<int,int>::iterator it;
        for(int i=0;i<nums.size();i++){
            it=CompSum.find(target-nums[i]);
            if(it==CompSum.end()){
                CompSum[nums[i]]=i;
            }
            else{
                return {it->second+1,i+1};
            }
        }
    return {};
    }
};
