class Solution {
public:
    int searchInsert(vector<int>& nums, int target) {
        int i,l=0;
        for(i=0;i<nums.size()-1;i++){
            if(nums[i]==target){
                return i;
            }
            else if(target<nums[i]){
                return i;
            }
        }
        if(target==nums[i]){
            return i;
        }else if(target<nums[i]){
            return i;
        }
        
            return i+1;
    }
};
